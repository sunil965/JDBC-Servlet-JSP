package database;

import java.sql.DriverManager;

public class DbConnection {

	public void connect(){
		try {
			Class.forName("com.mysql.jdbc.Driver");
		} catch (ClassNotFoundException e) {
			e.printStackTrace();
		}
		Connection conn = DriverManager.getConnection("jdbc:mysql://localhost:3306/AddressBook",
				"root",
				"password");
	}
}
